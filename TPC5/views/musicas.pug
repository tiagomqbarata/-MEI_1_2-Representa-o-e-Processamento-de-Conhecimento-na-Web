extends layout

block content
    h1 Lista de Músicas
    h2 22 de março de 2022
    table
        tr
            th Título 
            th Músico 
            th Província
        each m in musicas 
            tr 
                td
                    a(href="/musicas/" + m.tit)= m.tit
                td= m.musico
                td
                    a(href="/musicas/prov/" + m.prov)= m.prov
